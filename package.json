{
  "name": "readerfront",
  "version": "3.5.0",
  "homepage": "https://github.com/dvaJi/ReaderFront/",
  "keywords": [
    "react",
    "styled-components",
    "standard-version",
    "manga",
    "manga-reader",
    "progressive-web-app"
  ],
  "author": "dvaJi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dvaJi/ReaderFront/issues"
  },
  "scripts": {
    "analyze": "yarn build && source-map-explorer build/static/js/main.*",
    "start": "yarn build:client && yarn start:server",
    "start:server": "nodemon --exec babel-node -- src/index.js",
    "start:prod": "npm run build:prod && npm run start:server:prod",
    "start:server:prod": "node build/index.js",
    "build:prod": "npm run build:client:prod && npm run build:server:prod",
    "build:client": "webpack -d",
    "build:server:prod": "babel src -s -D -d build",
    "build:client:prod": "webpack -p",
    "test": "jest --env=jsdom",
    "test:ci": "jest --env=jsdom --coverage",
    "release": "standard-version",
    "manage:translations": "node ./translationRunner.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.2",
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "ip": "^1.1.5",
    "js-cookie": "^2.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-dropzone": "^5.0.1",
    "react-ga": "^2.5.3",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.4.0",
    "react-intl-redux": "^2.0.2",
    "react-lazyload": "^2.3.0",
    "react-markdown": "^3.6.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-slick": "^0.23.1",
    "react-test-renderer": "^16.4.2",
    "reactstrap": "^6.4.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "sinon": "^6.2.0",
    "styled-components": "^3.4.5"
  },
  "devDependencies": {
    "@anilanar/moxios": "^0.9.0",
    "@types/react-intl": "^2.3.10",
    "autoprefixer": "^9.1.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^1.0.0",
    "dotenv-webpack": "^1.5.7",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-react-intl": "^2.0.1",
    "hard-source-webpack-plugin": "^0.12.0",
    "mini-css-extract-plugin": "^0.4.2",
    "nodemon": "^1.18.4",
    "postcss-loader": "^3.0.0",
    "react-intl-translations-manager": "^5.0.3",
    "redux-devtools": "^3.4.1",
    "redux-devtools-extension": "~2.13.5",
    "redux-mock-store": "^1.5.3",
    "source-map-explorer": "^1.6.0",
    "standard-version": "^4.4.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0"
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:8000",
      "ws": true
    },
    "/works": {
      "target": "http://localhost:8000",
      "ws": true
    },
    "/images": {
      "target": "http://localhost:8000",
      "ws": true
    }
  }
}
